{# templates/calendar/index.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
    <h1>Select an Apartment</h1>
    <ul>
        {% for apartment in apartments %}
            <li>
                <a href="{{ path('app_calendar_id', {'apartmentId': apartment.id}) }}">{{ apartment.titre }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
